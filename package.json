    "webpack-dev-server": "^2.9.3",
    "node-localstorage": "^1.3.0"
  },
  "dependencies": {
    "causality-redux": "^2.1.16",
    "google-libphonenumber": "^3.0.9",
    "material-ui": "^0.19.4",
    "react": "^16.0.0",
    "react-causality-redux": "^2.0.16",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^3.7.2"
  },
  "config": {
    "host": "localhost",
    "port": 3000,
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  }
}
