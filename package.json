{
  "name": "create-react-project",
  "version": "1.0.0",
  "description": "A Webpack tempplate with hot reloading React components with causality-redux for react, error handling on module and component level and Visual Studio Code integration",
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production webpack --colors --config devtools/webpack.config.prod.js",
    "builddll:dev": "webpack --colors --config devtools/webpack.config.dlllib.dev.js",
    "builddll:prod": "cross-env NODE_ENV=production webpack --colors --config devtools/webpack.config.dlllib.prod.js",
    "build": "npm run build:prod",
    "builddll": "npm run builddll:dev && npm run builddll:prod",
    "build:all": "npm run builddll:dev && npm run builddll:prod && npm run build",
    "start": "cross-env NODE_ENV=development node devtools/server.js",
    "startprod": "cross-env NODE_ENV=production node devtools/server.js",
    "startp": "node devtools/pserver.js",
    "lint": "eslint src/**",
    "test": "cross-env NODE_ENV=mochaTesting mocha --require babel-register --require ignore-styles --require test/setup.js test/projectsetup.js test/mountapp.jsx src/**/*.spec.js --no-timeouts",
    "createcomponent": "node devtools/createcomponent.js",
    "createcomponentwc": "node devtools/createcomponentwc.js",
    "mochadebug": "npm run compile:del && npm run compile:copy && npm run compile:src",
    "config": "node devtools/apps/server.js",
    "startjsonserver": "node jsonserver.js",
    "installproject": "npm install && npm run build:all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AndrewBanks10/create-react-project"
  },
  "keywords": [
    "causality-redux",
    "react-causality-redux",
    "react",
    "reactjs",
    "tempplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "enzyme",
    "mocha"
  ],
  "author": "Andrew Banks",
  "license": "MIT",
  "homepage": "https://github.com/AndrewBanks10/create-react-project",
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "awesome-typescript-loader": "^3.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-jsx-source": "^6.22.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "copyfiles": "^1.2.0",
    "core-js": "^2.5.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^4.10.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-lodash": "^2.5.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.3.0",
    "json-loader": "^0.5.7",
    "json-server": "^0.12.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lodash": "^4.17.4",
    "mocha": "^4.0.1",
    "node-sass": "^4.5.3",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.0.8",
    "react-hot-loader": "^3.1.1",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.6.1",
    "url-loader": "^0.6.2",
